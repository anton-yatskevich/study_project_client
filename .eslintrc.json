{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", "tab"],
        "no-tabs": 0,
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}